{
  "name": "vitamins-dashboard",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@reach/router": "^1.2.1",
    "date-fns": "^2.8.1",
    "date-fns-tz": "^1.0.8",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.3.0",
    "recharts": "^2.0.0-beta.1",
    "vitamins": "^1.0.7"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "run-p build:** && cp _redirects build/_redirects",
    "build:app": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
